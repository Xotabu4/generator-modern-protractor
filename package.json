{
  "name": "generator-modern-protractor",
  "version": "2.0.2",
  "description": "Yeoman generator for protractorJS projects, with TypeScript, async/await, PageFragments, nice console and junit.xml output, screenshots on failure, JasmineJS, matchers for elements",
  "homepage": "https://xotabu4.github.io/generator-modern-protractor/",
  "author": {
    "name": "Oleksandr Khotemskyi",
    "email": "xotabu4@gmail.com",
    "url": "xotabu4.github.io"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "protractorjs",
    "protractor",
    "generator",
    "typescript",
    "async/await",
    "yeoman-generator",
    "selenium",
    "webdriverjs"
  ],
  "devDependencies": {
    "yeoman-test": "^1.9.1",
    "yeoman-assert": "^3.1.1",
    "eslint": "^6.2.1",
    "prettier": "^1.15.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0",
    "jest": "^23.6.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "Xotabu4/generator-modern-protractor",
  "license": "MIT"
}
